  <script>
    // Get the current year for the copyright
    $('#year').text(new Date().getFullYear());

    // Configure Slider
    $('.carousel').carousel({
      interval: 1000000,
      pause: 'hover'
    });

    // Lightbox Init
    $(document).on('click', '[data-toggle="lightbox"]', function (event) {
      event.preventDefault();
      $(this).ekkoLightbox();
    });

    //Confirmation of registration


    // Video Play
    $(function () {
      // Auto play modal video
      $(".video").click(function () {
        var theModal = $(this).data("target"),
          videoSRC = $(this).attr("data-video"),
          videoSRCauto = videoSRC +
            "?modestbranding=1&rel=0&controls=0&showinfo=0&html5=1&autoplay=1";
        $(theModal + ' iframe').attr('src', videoSRCauto);
        $(theModal + ' button.close').click(function () {
          $(theModal + ' iframe').attr('src', videoSRC);
        });
      });
    });
  </script>
  <script type="text/javascript">
    $(function () {
      if (mixpanel && mixpanel.track)
        mixpanel.track("mainPageView");
      $("#validate").click(function (e) {
        event.preventDefault();
        var userNameBox = document.getElementById('username').value;
        var passwordBox = document.getElementById('password').value;
        var data = {
          userName: userNameBox,
          password: passwordBox
        }
        $.post("signin", data, function (response) {
          if (response.status == "200") {
            sessionStorage.setItem("userLogIn", "true");
            window.location.href = '/form/' + response.data;
          } else {
            sessionStorage.setItem("userLogIn", "false");
            alert("Fail to signin, please try again later");
          }

        });
      })
      $("#addme").click(function (event) {
        if (mixpanel && mixpanel.track)
          mixpanel.track("sendEmailEvent");
      });
    });
  </script>
